<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="description" content="">

    <title>
        @if (string.IsNullOrEmpty(ViewBag.Title))
        {
            <text>OurDilemmas</text>
        }
        else
        {
            <text>OurDilemmas - </text> @ViewBag.Titles
        }
    </title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
    <!--icons-->
    <link rel="apple-touch-icon" sizes="57x57" href="/Content/images/icons/fav/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/Content/images/icons/fav/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/Content/images/icons/fav/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/Content/images/icons/fav/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/Content/images/icons/fav/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/Content/images/icons/fav/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/Content/images/icons/fav/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/Content/images/icons/fav/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/Content/images/icons/fav/apple-touch-icon-180x180.png">
    <link rel="icon" type="image/png" href="/Content/images/icons/fav/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/Content/images/icons/fav/android-chrome-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="/Content/images/icons/fav/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="/Content/images/icons/fav/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/Content/images/icons/fav/manifest.json">
    <link rel="shortcut icon" href="/Content/images/icons/fav/favicon.ico">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="/Content/images/icons/fav/mstile-144x144.png">
    <meta name="msapplication-config" content="/Content/images/icons/fav/browserconfig.xml">
    <meta name="theme-color" content="#000000">


    <!-- send jquery to the foot of the page -->
    <script type='text/javascript'>window.q = []; window.$ = function (f) { q.push(f); }</script>

</head>
<body>
    @Html.Partial("Header")
    
    @Html.Partial("Modals")
    
    @if (ViewBag.SuppressPageBd == true)
    {
        @RenderBody()
    }
    else
    {
        <div class="page-bd">
            <div class="wrap">
                @RenderBody()
            </div>
        </div>
    }


    
    @Html.Partial("Footer")
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/cookiedirective")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/homegrown")
    
    
    @RenderSection("scripts", required: false)

<script type="text/javascript">$.each(q, function(i, f) { $(f); })</script>
<script type="text/javascript">
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-63951801-1', 'auto');
    ga('send', 'pageview');
</script>
</body>
</html>
